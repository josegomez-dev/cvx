# Site URL (for metadata)
NEXT_PUBLIC_SITE_URL=https://your-domain.com

# Medium RSS Feed URL
MEDIUM_RSS_URL=https://medium.com/feed/@alegomez

# Optional Social Media URLs
NEXT_PUBLIC_YOUTUBE_CHANNEL=https://youtube.com/@alegomez
